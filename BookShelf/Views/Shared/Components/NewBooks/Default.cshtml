@model  List<BookShelf.Models.Book>


    <table class="table table-bordered">
        <tr class="bg-info">
            <th>ID</th>
            <th>Book Name</th>
            <th>Author</th>
        </tr>

        @foreach (var k in Model)
        {
            <tr>
                <td>@k.ID</td>
                <td>@k.BookName</td>
                <td>@k.Author</td>
            </tr>
        }

    </table>
